[user]
   name = Christopher MCAdams
   email = cmcadams@groupon.com
[web]
   browser = chrome
[push]
   default = simple
[alias]
   st = status
   co = checkout
   graph = 'git log --oneline --graph --abbrev-commit --pretty=format:"%Cgreen %h %Creset%s %n%an - %cr %n%n"'
   mismatch = ls-files -u
   del = branch -D
   remdel = push origin --delete
   updel = push upstream --delete
   pullr = pull --rebase
   b = branch
[help]
   browser = chrome
[core]
  editory = vim
  fileMode = false
  autocrlf = input
  safecrlf = false
  excludesfile = /Users/chrismcadams/.gitignore_global
  gitproxy = gitproxy for github
[credential]
   helper = osxkeychain
[diff]
	tool = vimdiff
[difftool]
	prompt = true
[color]
	ui = true
